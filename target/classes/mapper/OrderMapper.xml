<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.mysiteforme.admin.dao.OrderDao" >

    <select id="selectDetailOrder" resultType="com.mysiteforme.admin.entity.Order" parameterType="java.util.Map">
        select
       *
       from
       order_list
       where
        del_flag='0'
    </select>



</mapper>